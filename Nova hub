})

local Button = Tab3:CreateButton({
   Name = "inf stamina",
   Callback = function()
       local args = {
    [1] = 0/0
}

game:GetService("ReplicatedStorage").Packages.Knit.Services.StaminaService.RE.DecreaseStamina:FireServer(unpack(args))
   end,
})

